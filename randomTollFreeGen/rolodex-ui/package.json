{
  "name": "rolodex-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy-dev": "aws cloudformation deploy --template-file ../cloudfn/app-hosting.yml --stack-name rolodex-ui --parameter-overrides BucketName=rolodex-ui-dev --profile sleethma",
    "upload-ui": "aws s3 sync build/ s3://rolodex-ui-dev --delete --profile sleethma",
    "deploy-ui": "npm run build && npm run upload-ui",
    "test-bash": "chmod +x ./test-script.sh && ./test-script.sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
